{
  "name": "ingress-prime-stat",
  "description": "A lightweight conversion for Ingress Prime stat in TypeScript",
  "version": "0.0.0-development",
  "main": "src/main.ts",
  "author": "haloivanid <haloivan.idn@gmail.com>",
  "repository": {
    "url": "https://github.com/haloivanid/ingress-prime-stat.git",
    "type": "git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.16.0",
    "npm": ">=9.5.1",
    "yarn": ">=1.22.19"
  },
  "scripts": {
    "prebuild": "npx rimraf ./lib",
    "build": "tsc -p tsconfig.json",
    "postbuild": "terser ./lib/main.js -c -m -o ./lib/main.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/main.ts",
    "test": "jest",
    "lint": "eslint --ext .ts src --fix",
    "format": "prettier --write \"**/*.ts\"",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "dayjs": "^1.11.9"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.0.4",
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.2",
    "prettier": "^3.0.1",
    "semantic-release": "^21.0.7",
    "terser": "^5.19.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
